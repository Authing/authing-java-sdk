<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.authing.testdemo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.authing.testdemo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity"/>
        <activity android:name=".activity.LoginEmailActivity"/>
        <activity android:name=".activity.LoginPhoneActivity"/>
        <activity android:name=".activity.RefreshTokenActivity"/>
        <activity android:name=".activity.UserInfoActivity"/>
        <activity android:name=".activity.RemoveUserActivity" />
        <activity android:name=".activity.UpdateUserInfoActivity" />
        <activity android:name=".activity.ResetPsdActivity" />
        <activity android:name=".activity.VerifyEmailActivity" />
        <activity android:name=".activity.UnbindEmailActivity" />
        <activity android:name=".activity.BindThirdPartyOAuthActivity" />
        <activity android:name=".activity.RoleActivity" />
        <activity android:name=".activity.UserPermissionActivity" />
        <activity android:name=".activity.SigninActivity" />
    </application>

</manifest>